<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btStepView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="StepView" />

    <Button
        android:id="@+id/btEvaluationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="EvaluationView" />

    <Button
        android:id="@+id/btCouponLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="CouponLayout" />
    <Button
        android:id="@+id/btCouponDetailLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="CouponDetailLayout" />



</LinearLayout>